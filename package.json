{
  "name": "@canonical/jujulib",
  "version": "1.3.2",
  "description": "Juju API client",
  "main": "api/client.js",
  "type": "module",
  "scripts": {
    "build": "yarn run build-generator && yarn run generate && yarn run build-facades",
    "build-generator": "tsc -t ES2019 generator/index.ts",
    "build-facades": "tsc -t ES2019 api/facades/*.ts",
    "lint": "eslint api/",
    "test": "tap 'api/test-!(helpers)*.js'",
    "watch": "tsc --watch -t ES2019 --moduleResolution Node --allowSyntheticDefaultImports --sourceMap generator/index.ts",
    "generate": "./generate.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/juju/js-libjuju.git"
  },
  "keywords": [
    "juju",
    "api",
    "client"
  ],
  "author": "Jeff Pihach <jeff.pihach@canonical.com> (https://fromanegg.com)",
  "license": "LGPL-3.0",
  "bugs": {
    "url": "https://github.com/juju/js-libjuju/issues"
  },
  "homepage": "https://github.com/juju/js-libjuju#readme",
  "devDependencies": {
    "@types/node": "^14.0.27",
    "btoa": "^1.2.1",
    "documentation": "^13.0.2",
    "eslint": "^7.6.0",
    "prettier": "^2.0.5",
    "sinon": "^9.0.3",
    "tap": "^14.10.8",
    "typescript": "^3.9.7",
    "websocket": "^1.0.26",
    "xhr2": "^0.2.0"
  },
  "dependencies": {
    "@canonical/macaroon-bakery": "^0.3.0"
  }
}
